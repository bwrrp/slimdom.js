{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug Tests",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/node_modules/jest/bin/jest.js",
			"stopOnEntry": false,
			"args": [
				"--runInBand",
				"temp/wpt/dom/ranges/Range-isPointInRange.html"
			],
			"env": {
				"WEB_PLATFORM_TESTS_PATH": "${workspaceRoot}/temp/wpt"
			},
			"runtimeArgs": [
				"--inspect=:9070"
			],
			"sourceMaps": true,
			"port": 9070,
			"protocol": "inspector",
			"console": "integratedTerminal"
		}
	]
}
